  },
  "dependencies": {
    "techbook-template-cli": "github:kght6123/techbook-template-cli#v0.10-Release"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3"
  },
  "volta": {
    "node": "20.11.1"
  }
}
